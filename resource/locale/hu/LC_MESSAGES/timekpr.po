msgstr "Napok & korlátok"

#: resource/client/forms/client.glade:511
msgid "This shows time intervals that are available for Your use"
msgstr "Rendelkezésre álló időintervallumok"

#: resource/client/forms/client.glade:536
msgid "Intervals"
msgstr "Időközök"

#: resource/client/forms/client.glade:589
msgid "Additional statistics"
msgstr "További statisztikák"

#: resource/client/forms/client.glade:610
msgid "TIme spent for this week"
msgstr "Ezen a héten felhasznált idő"

#: resource/client/forms/client.glade:639
msgid "Time spent for this month"
msgstr "Ebben a hónapban felhasznált idő"

#: resource/client/forms/client.glade:656
msgid "Track inactive:"
